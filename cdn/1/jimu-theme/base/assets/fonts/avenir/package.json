{
  "name": "calcite-fonts",
  "version": "2.1.1",
  "description": "CSS and font files for self-hosting Esri branded typography",
  "main": "index.js",
  "scripts": {
    "prestart": "rimraf fonts.css && rimraf fonts/fonts.css",
    "start": "node-sass fonts.scss fonts.css --output-style compressed",
    "poststart": "node-sass cdn.scss fonts/fonts.css --output-style compressed"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/ArcGIS/calcite-fonts.git"
  },
  "keywords": [
    "fonts",
    "css",
    "calcite-web"
  ],
  "author": "Paul C Pederson <ppederson@esri.com>",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/ArcGIS/calcite-fonts/issues"
  },
  "homepage": "https://github.com/ArcGIS/calcite-fonts#readme",
  "devDependencies": {
    "node-sass": "^5.0.0",
    "rimraf": "^2.6.2"
  }
}
