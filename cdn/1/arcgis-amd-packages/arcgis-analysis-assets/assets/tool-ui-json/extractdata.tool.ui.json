{
  "toolName": "ExtractData",
  "version": "0.1.0",
  "rules": [
    {
      "ruleType": "equals",
      "valueA": "$[Data].extent",
      "valueB": null,
      "destination": "clip",
      "effectParams": {
        "visible": false
      }
    },
    {
      "ruleType": "set",
      "value": "$[Component].dataFormat.value",
      "destination": "resultType",
      "property": "value"
    }
  ],
  "UIparameters": [
    {
      "label": "$inputLayersGroupLabel",
      "componentName": "analysis-block",
      "summaryText": "$inputLayersSectionText",
      "UIparameters": [
        {
          "name": "inputLayers",
          "label": "$inputLayersGroupLabel",
          "componentName": "analysis-layer-input",
          "layerFilterType": ["extract"],
          "selectionMode": "multi",
          "hideSketch": true,
          "required": true
        }
      ]
    },
    {
      "label": "$extentSectionLabel",
      "componentName": "analysis-block",
      "summaryText": "$extentSectionText",
      "UIparameters": [
        {
          "name": "extent",
          "label": "$extentLayer",
          "componentName": "analysis-layer-input",
          "layerFilterType": ["feature"]
        },
        {
          "name": "clip",
          "label": "$clipFeatures",
          "componentName": "analysis-boolean-input"
        }
      ]
    },
    {
      "label": "$resultLayerGroupLabel",
      "componentName": "analysis-block",
      "summaryText": "$outputAreasSectionText",
      "UIparameters": [
        {
          "name": "dataFormat",
          "label": "$dataFormat",
          "defaultValue": "CSV",
          "choiceList": ["CSV", "FileGeodatabase", "ShapeFile", "KML"],
          "choiceListLabels": {
            "CSV": "$csv",
            "FileGeodatabase": "$fgdb",
            "ShapeFile": "$shp",
            "KML": "$kml"
          },
          "choiceListIcons": {
            "CSV": "file-csv",
            "FileGeodatabase": "file-data",
            "ShapeFile": "file-shape",
            "KML": "file-kml"
          },
          "componentName": "analysis-string-input",
          "required": true
        },
        {
          "name": "resultType",
          "label": "$dataFormat",
          "componentName": "analysis-string-input",
          "visible": false
        },
        {
          "name": "outputName",
          "label": "$outputName",
          "placeholderText": "$outputNamePlaceholderText",
          "componentName": "analysis-string-input",
          "required": true
        },
        {
          "name": "saveResultIn",
          "label": "$saveResultIn",
          "componentName": "analysis-folder-input"
        }
      ]
    }
  ]
}
